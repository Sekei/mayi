<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/rl_match_parent"
    android:background="@color/color_000">

    <android.support.v7.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        app:cardElevation="0px"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="false">

        <RelativeLayout style="@style/rl_match_parent">

            <com.mayi.video.widget.VerticalViewPager
                android:id="@+id/viewpager"
                style="@style/view_match_parent" />

            <com.mayi.video.utils.CompatToolbar style="@style/view">

                <RelativeLayout style="@style/rl">


                </RelativeLayout>
            </com.mayi.video.utils.CompatToolbar>
        </RelativeLayout>
    </android.support.v7.widget.CardView>
</RelativeLayout>
